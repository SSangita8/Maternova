    "foods_to_avoid": [
        {
            "category": "Raw or Undercooked Foods",
            "foods": [
                "Raw meat (including sushi, undercooked poultry)",
                "Raw eggs",
                "Raw fish"
            ],
            "reason": "Risk of foodborne illnesses like salmonella, listeria, toxoplasmosis."
        },
        {
            "category": "High Mercury Fish",
            "foods": [
                "Shark",
                "Swordfish",
                "King mackerel",
                "Tilefish"
            ],
            "reason": "Mercury can harm the baby's developing nervous system."
        },
        {
            "category": "Unpasteurized Dairy Products",
            "foods": [
                "Raw milk",
                "Soft cheeses like brie, feta, camembert (unless made with pasteurized milk)"
            ],
            "reason": "Risk of listeria infection."
        },
        {
            "category": "Certain Seafood",
            "foods": [
                "Smoked seafood (unless cooked)",
                "Refrigerated seafood spreads"
            ],
            "reason": "Risk of listeria contamination."
        },
        {
            "category": "Processed Meats",
            "foods": [
                "Deli meats (unless heated until steaming hot)",
                "Hot dogs",
                "Sausages"
            ],
            "reason": "Risk of listeria and high levels of nitrates."
        },
        {
            "category": "Raw Sprouts",
            "foods": [
                "Alfalfa",
                "Mung bean",
                "Radish sprouts"
            ],
            "reason": "Can be contaminated with bacteria like E. coli and salmonella."
        },
        {
            "category": "Unwashed Fruits and Vegetables",
            "foods": [
                "Unwashed produce"
            ],
            "reason": "Risk of toxoplasmosis from soil contamination."
        },
        {
            "category": "Caffeine",
            "foods": [
                "Excessive coffee",
                "Energy drinks",
                "High-caffeine tea"
            ],
            "reason": "High levels of caffeine can lead to low birth weight and preterm birth."
        },
        {
            "category": "Alcohol",
            "foods": [
                "All alcoholic beverages"
            ],
            "reason": "Can lead to fetal alcohol spectrum disorders (FASD)."
        },
        {
            "category": "Artificial Sweeteners",
            "foods": [
                "Saccharin",
                "Cyclamate"
            ],
            "reason": "Some studies suggest potential risks, though data is inconclusive."
        },
        {
            "category": "Excessive Sugar",
            "foods": [
                "Sweets",
                "Sugary drinks"
            ],
            "reason": "Can lead to excessive weight gain, gestational diabetes."
        },
        {
            "category": "Excessive Salt",
            "foods": [
                "Processed snacks",
                "Fast food"
            ],
            "reason": "Can contribute to high blood pressure and swelling."
        },
        {
            "category": "Certain Herbal Teas",
            "foods": [
                "Licorice tea",
                "Peppermint tea in large quantities"
            ],
            "reason": "Some herbs can induce contractions or other complications."
        },
        {
            "category": "Unripe Papaya",
            "foods": [
                "Unripe or semi-ripe papaya"
            ],
            "reason": "Contains latex, which can trigger uterine contractions."
        },
        {
            "category": "Liver Products",
            "foods": [
                "Liver",
                "Liver p\u00e2t\u00e9"
            ],
            "reason": "High in vitamin A, which can cause birth defects if consumed in excess."
        },
        {
            "category": "Certain Homemade Products",
            "foods": [
                "Homemade mayonnaise (if made with raw eggs)",
                "Homemade ice cream"
            ],
            "reason": "Risk of salmonella from raw eggs."
        }
    ],
